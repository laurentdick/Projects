<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="PapyrusSqlServer"
         connectionString="Data Source=(local)\SQLEXPRESS;Initial Catalog=Papyrus;Integrated Security=True;Connect Timeout=3"
         providerName="System.Data.SqlClient" />
    <add name="PapyrusMySql"
         connectionString="Server=localhost;Database=Papyrus; User=root;Password=" />
  </connectionStrings>
  <appSettings>
    <add key ="factorySqlServer" value="System.Data.SqlClient"/>
    <add key ="factoryMySql" value="MySql.Data.MySqlClient"/>
  </appSettings>
</configuration>